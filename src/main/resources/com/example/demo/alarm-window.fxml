<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="167.0" prefWidth="194.0" style="-fx-background-color: #E0FFFF;"
            xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.demo.AlarmAlertController">

    <Label fx:id="alertMessageLabel" layoutX="40.0" layoutY="37.0" text="Alarm!" textFill="#188080">
        <font>
        <Font size="40.0"/>
        </font>
    </Label>
    <Button layoutX="52.0" layoutY="117.0" onAction="#handleOkButton" prefHeight="25.0" prefWidth="91.0"
            style="-fx-background-color: #20B2AA;" text="OK" textFill="LIGHTCYAN">
        <font>
        <Font name="Palatino Linotype" size="14.0"/>
        </font>
    </Button>
</AnchorPane>


